{
  "name": "react-redux-realworld-example-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@cypress/grep": "^4.1.0",
    "cross-env": "^5.1.4",
    "cypress": "^14.5.1",
    "cypress-mochawesome-reporter": "^4.0.0",
    "cypress-terminal-report": "^7.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "react-scripts": "1.1.1",
    "start-server-and-test": "^2.0.12",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "history": "^4.6.3",
    "marked": "^0.3.6",
    "prop-types": "^15.5.10",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.1",
    "superagent": "^3.8.2",
    "superagent-promise": "^1.1.0"
  },
  "scripts": {
    "start": "cross-env PORT=4100 react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env PORT=4100 react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "cypress:run:smoke": "cypress run --env grep=@smoke",
    "cypress:run:auth": "cypress run --env grep=@auth",
    "cypress:run:ui": "cypress run --env grep=@ui",
    "cypress:run:api": "cypress run --env grep=@api",
    "cypress:run:integration": "cypress run --env grep=@integration",
    "cypress:run:parallel": "cypress run --record --parallel",
    "cypress:run:headed": "cypress run --headed",
    "cypress:run:spec": "cypress run --spec",
    "test:e2e": "start-server-and-test start http://localhost:4100 cypress:run",
    "test:e2e:open": "start-server-and-test start http://localhost:4100 cypress:open",
    "test:e2e:smoke": "start-server-and-test start http://localhost:4100 cypress:run:smoke",
    "test:e2e:auth": "start-server-and-test start http://localhost:4100 cypress:run:auth",
    "test:e2e:ui": "start-server-and-test start http://localhost:4100 cypress:run:ui",
    "test:e2e:api": "start-server-and-test start http://localhost:4100 cypress:run:api",
    "test:e2e:integration": "start-server-and-test start http://localhost:4100 cypress:run:integration",
    "test:e2e:parallel": "start-server-and-test start http://localhost:4100 cypress:run:parallel",
    "test:e2e:chrome": "start-server-and-test start http://localhost:4100 cypress:run:chrome",
    "test:e2e:firefox": "start-server-and-test start http://localhost:4100 cypress:run:firefox",
    "test:e2e:edge": "start-server-and-test start http://localhost:4100 cypress:run:edge",
    "test:component": "cypress run --component",
    "test:component:open": "cypress open --component",
    "test:all": "npm run test:e2e && npm run test:component",
    "ci:test": "npm run build && npm run test:e2e",
    "ci:test:parallel": "npm run build && npm run test:e2e:parallel"
  }
}
